<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph_onboarding"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.fx_trading.lessons.feature_onboarding.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/action_splashFragment_to_onBoardingFragmentFirst"
            app:destination="@id/onBoardingFragmentFirst"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_splashFragment_to_welcomeFragment"
            app:destination="@id/test_skils_fragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true"/>

    </fragment>
    <fragment
        android:id="@+id/onBoardingFragmentFirst"
        android:name="com.fx_trading.lessons.feature_onboarding.ui.OnBoardingFirstFragment"
        android:label="fragment_on_boarding_first"
        tools:layout="@layout/fragment_on_boarding_first">
        <action
            android:id="@+id/action_onBoardingFragmentFirst_to_OnBoardingFragmentSecond"
            app:destination="@id/onBoardingFragmentSecond"
            app:popUpTo="@id/onBoardingFragmentFirst"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_onBoardingFragmentFirst_to_welcomeFragment"
            app:destination="@id/test_skils_fragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true"/>
    </fragment>

    <fragment
        android:id="@+id/onBoardingFragmentSecond"
        android:name="com.fx_trading.lessons.feature_onboarding.ui.OnBoardingSecondFragment"
        android:label="fragment_on_boarding_second"
        tools:layout="@layout/fragment_on_boarding_second">
        <action
            android:id="@+id/action_onBoardingFragmentSecond_to_WelcomeFragment"
            app:destination="@id/test_skils_fragment"
            app:popUpTo="@id/onBoardingFragmentFirst"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_onBoardingFragmentSecond_to_test_skils_fragment"
            app:destination="@id/test_skils_fragment" />
    </fragment>

    <fragment
        android:id="@+id/onBoardingFragmentThird"
        android:name="com.fx_trading.lessons.feature_onboarding.ui.OnBoardingThirdFragment"
        android:label="fragment_on_boarding_third"
        tools:layout="@layout/fragment_on_boarding_third"/>

    <fragment
        android:id="@+id/onBoardingFragmentFour"
        android:name="com.fx_trading.lessons.feature_onboarding.ui.OnBoardingFourFragment"
        android:label="fragment_on_boarding_four"
        tools:layout="@layout/fragment_on_boarding_four"/>

    <fragment
        android:id="@+id/onBoardingFragmentFive"
        android:name="com.fx_trading.lessons.feature_onboarding.ui.OnBoardingFiveFragment"
        android:label="fragment_on_boarding_five"
        tools:layout="@layout/fragment_on_boarding_four"/>

    <fragment
        android:id="@+id/test_skils_fragment"
        android:name="com.fx_trading.lessons.feature_onboarding.start_test.StartTestFragment"
        android:label="fragment_test"
        tools:layout="@layout/fragment_start_test">

    </fragment>
<!--    <fragment-->
<!--        android:id="@+id/testFragment"-->
<!--        android:name="com.fx_trading.lessons.feature_onboarding.test.TestFragment"-->
<!--        android:label="TestFragment" />-->


</navigation>